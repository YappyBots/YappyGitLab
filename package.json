  "dependencies": {
    "@YappyBots/addons": "github:YappyBots/yappy-addons",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.1.1",
    "chalk": "^4.0.0",
    "clean-stack": "^2.2.0",
    "clean-stacktrace": "^1.1.0",
    "cookie-parser": "^1.4.5",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^10.7.0",
    "hbs": "^4.1.1",
    "jsondiffpatch": "^0.4.1",
    "knex": "^0.20.13",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "node-fetch": "^2.6.1",
